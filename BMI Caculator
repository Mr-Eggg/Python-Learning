height = float(input("enter your height in m: "))
weight = float(input("enter your weight in kg: "))
# ğŸš¨ Don't change the code above ğŸ‘†

#Write your code below this line ğŸ‘‡
bmi=weight/height**2
bmi_shown=round(weight/height**2,0)
if bmi<18.5:
    print(f"Your bmi is {bmi_shown}, you are underweight.")
elif bmi<25:
  print(f"Your bmi is {bmi_shown}, you have a normal weight.")
elif bmi<30:
  print(f"Your bmi is {bmi_shown}, you are slightly overweight")
elif bmi<35:
  print(f"Your bmi is {bmi_shown}, you are obese.")
else:
  print(f"Your bmi is {bmi_shown}, you are clinically obesee.")
